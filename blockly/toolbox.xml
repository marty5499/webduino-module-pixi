<category id="catVideoSensor">
  <block type="waspot_get_camera">
  </block>
  <block type="pixi_game">
  </block>

  <block type="pixi_actor">
    <field name="stage">stage</field>
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
    <value name="imgURL">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>
  </block>


  <block type="pixi_actor_size">
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
    <value name="width">
      <block type="math_number">
        <field name="NUM">32</field>
      </block>
    </value>
    <value name="height">
      <block type="math_number">
        <field name="NUM">32</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_pos">
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
    <value name="x">
      <block type="math_number">
        <field name="NUM">0</field>
      </block>
    </value>
    <value name="y">
      <block type="math_number">
        <field name="NUM">0</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_move">
    <field name="destroy">FALSE</field>
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
    <value name="x">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="y">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="speed">
      <block type="math_number">
        <field name="NUM">3</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_move_cb">
    <field name="destroy">FALSE</field>
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
    <value name="x">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="y">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="speed">
      <block type="math_number">
        <field name="NUM">3</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_collision">
    <value name="collision">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_collision_obj">
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
  </block>

  <block type="pixi_actor_destroy">
    <value name="actor">
      <block type="variables_get">
        <field name="VAR">actor</field>
      </block>
    </value>
  </block>

  <block type="pixi_pipe_outofbound">
    <value name="pipe">
      <block type="variables_get">
        <field name="VAR">pipe</field>
      </block>
    </value>
    <value name="x">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="y">
      <block type="math_number">
        <field name="NUM">100</field>
      </block>
    </value>
    <value name="r">
      <block type="math_number">
        <field name="NUM">15</field>
      </block>
    </value>
  </block>


  <block type="pixi_pipe_create">
    <field name="color">#ffcc33</field>
    <value name="pipe">
      <block type="variables_get">
        <field name="VAR">pipe</field>
      </block>
    </value>
    <value name="data">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>
    <value name="stage">
      <block type="variables_get">
        <field name="VAR">stage</field>
      </block>
    </value>
  </block>
</category>